<mvc:View controllerName="com.xtendhr.web.controller.WebService"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page_call" title="{i18n>title}" navButtonPress="onNavBack" showNavButton="true">
        <content>
            <Panel id="pnlId1" expand="true" headerText="Web Services Application" width="auto" class="sapUiResponsiveMargin" expandable="false">
                <content>
                    <Label id="idInpLbl" text="Get Service Path: "/> <Input id="idInput"></Input>                    
                    <Button id="idBtn" press="onCallSRV" text="Call Service"/>
                    <TextArea id="idTextarea" growing="true" width="100%"/>
                </content>
			</Panel>
        </content>
    </Page>
</mvc:View>
